{
  "family": "sandbox-manager-prototype-stage",
  "containerDefinitions": [
    {
      "name": "sandbox-manager-prototype-stage",
      "image": "hspconsortium/sandbox-manager-prototype:{{PROJECT_VERSION}}",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "memoryReservation": 100,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/sandbox-manager-prototype-stage",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "essential": true,
      "environment": [
        {
          "name": "JASYPT_ENCRYPTOR_PASSWORD",
          "value": ""
        },
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": ""
        },
        {
          "name": "TARGET_ENV",
          "value": "prod"
        }
      ]
    }
  ]
}
