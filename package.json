{
  "name": "react-sandbox-manager",
  "version": "0.0.1",
  "description": "HSPC Sandbox Manager",
  "author": "Interopion",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "scripts": {
    "test": "npm run sandbox-manager:test",
    "coverage": "npm run sandbox-manager:coverage",
    "sandbox-manager:install": "cd projects/sandbox-manager && npm install && cd ../..",
    "sandbox-manager:build:local": "cd projects/sandbox-manager && npm run clean && NODE_ENV=local webpack",
    "sandbox-manager:build:dev": "cd projects/sandbox-manager && npm run clean && NODE_ENV=development webpack",
    "sandbox-manager:build:uat": "cd projects/sandbox-manager && npm run clean && NODE_ENV=uat webpack",
    "sandbox-manager:build:production": "cd projects/sandbox-manager && npm run clean && NODE_ENV=production webpack",
    "sm": "npm run sandbox-manager:start:local",
    "sandbox-manager:start:local": "cd projects/sandbox-manager && npm run start:local",
    "smt": "npm run sandbox-manager:test",
    "sandbox-manager:test": "NODE_ENV=test mocha --opts mocha.opts",
    "smtw": "npm run sandbox-manager:test:watch",
    "sandbox-manager:test:watch": "NODE_ENV=test mocha --opts mocha.opts --watch",
    "sandbox-manager:test:coverage": "npm run sandbox-manager:clean:tests_reports && NODE_ENV=test nyc --report-dir projects/sandbox-manager/test_reports/coverage mocha --opts mocha.cov.opts --reporter mochawesome --reporter-options reportDir=projects/sandbox-manager/test_reports/tests,reportFilename=index",
    "sandbox-manager:clean:tests_reports": "rm -rf projects/sandbox-manager/test_reports"
  },
  "pre-push": [
    "coverage",
    "sandbox-manager:check:coverage"
  ],
  "nyc": {
    "include": [],
    "exclude": [
      "coverage/*.js",
      "node_modules",
      "postcss.config.js",
      "projects/sandbox-manager/webpack.config.js",
      "projects/sandbox-manager/build/www/*.js",
      "projects/sandbox-manager/build/www/externals/*.js",
      "projects/sandbox-manager/externals/*.js",
      "**/__test__",
      "__storage__"
    ],
    "extension": [
      ".js",
      ".jsx"
    ],
    "reporter": [
      "text-summary",
      "html",
      "clover",
      "json-summary"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": false
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-jest": "20.0.3",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "chartkick": "^2.2.4",
    "dotenv": "4.0.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "highcharts": "^6.0.6",
    "immutable": "^3.8.2",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-chartkick": "^0.1.6",
    "react-cookie": "^2.1.2",
    "react-cookies": "^0.1.0",
    "react-dev-utils": "^4.2.1",
    "react-highcharts": "^16.0.2",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack-manifest-plugin": "1.3.2",
    "babel-polyfill": "^6.26.0",
    "fhirclient": "^0.1.12",
    "font-awesome": "^4.7.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.20.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
